<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingEnd="20dp"
    android:paddingStart="24dp"
    >

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="9dp"
        android:paddingTop="9dp" >

        <TextView
            android:id="@+id/tvSettleNo"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Bd23fss2342"
            android:textColor="@color/base_orange" />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="9dp"
        android:paddingTop="9dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="流水："
            android:textColor="@color/base_black_333333" />

        <TextView
            android:id="@+id/tvSerialAmount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2015-07-23"
            android:textColor="@color/base_black_333333" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="9dp"
        android:paddingTop="9dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="现金流水："
            android:textColor="@color/base_black_333333" />

        <TextView
            android:id="@+id/tvCashAmount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2015-07-23"
            android:textColor="@color/base_black_333333" />
    </TableRow>
    
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="9dp"
        android:paddingTop="9dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="在线流水："
            android:textColor="@color/base_black_333333" />

        <TextView
            android:id="@+id/tvOnlineAmount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2015-07-23"
            android:textColor="@color/base_black_333333" />
    </TableRow>
    
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="9dp"
        android:paddingTop="9dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="服务费："
            android:textColor="@color/base_black_333333" />

        <TextView
            android:id="@+id/tvServiceFee"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2015-07-23"
            android:textColor="@color/base_black_333333" />
    </TableRow>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="账号信息" />

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="9dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="户名："
            android:textColor="@color/base_black_333333" />

        <TextView
            android:id="@+id/tvBankAccountName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="huantu"
            android:textColor="@color/base_black_333333" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="账号："
            android:textColor="@color/base_black_333333" />

        <TextView
            android:id="@+id/tvBankAccount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="huantu_zhifubao"
            android:textColor="@color/base_black_333333" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="9dp"
        android:paddingTop="9dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="交易："
            android:textColor="@color/base_black_333333" />

        <TextView
            android:id="@+id/tvEndTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2015-07-23"
            android:textColor="@color/base_black_333333" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="9dp"
        android:paddingTop="9dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="备注："
            android:textColor="@color/base_black_333333" />

        <TextView
            android:id="@+id/tvComment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2015-07-23"
            android:textColor="@color/base_black_333333" />
    </TableRow>


</TableLayout>